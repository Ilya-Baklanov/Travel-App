{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {"modules": false} ]
  ],
  "plugins": [
    [
      "module-resolver", {
        "alias": {
          "@/": "src/"
        }
      },
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
